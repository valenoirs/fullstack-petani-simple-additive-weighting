<div class="container-fluid">
  <div class="row mt-5 px-5">
    <div class="col">
      <h3>Simple Additive Weighting</h3>

      <div class="text-center mt-5 mb-5">
        <% score.forEach((score, index) => { %> <% if (index === 0) { %>
        <h6>Kelompok Tani yang dipilih:</h6>
        <h3><%= index + 1 %>. <%= score.name %></h3>
        <% } %> <% }) %>
      </div>

      <table class="table table-hover">
        <thead>
          <tr>
            <th scope="col">Peringkat</th>
            <th scope="col">Kelompok Tani</th>
            <th scope="col">Score</th>
          </tr>
        </thead>
        <tbody>
          <% score.forEach((score, index) => { %>
          <tr>
            <th scope="row"><%= index + 1 %></th>
            <td><%= score.name %></td>
            <td><%= score.score %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>
